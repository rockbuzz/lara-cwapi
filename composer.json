{
  "name": "rockbuzz/lara-cwapi",
  "description": "Cloudways Api",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Rockbuzz\\LaraCwApi\\": "src/"
    },
    "files": [
      "src/Helpers/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.3",
    "guzzlehttp/guzzle": "^7.4"
  },
  "require-dev": {
    "orchestra/testbench": "^5.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "cs": "vendor/bin/phpcs"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rockbuzz\\LaraCwApi\\ServiceProvider"
      ]
    }
  }
}
